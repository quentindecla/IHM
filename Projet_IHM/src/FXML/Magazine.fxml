                <AnchorPane prefHeight="1000.0" prefWidth="1200.0">
                     <children>
                        <ImageView fitHeight="404.0" fitWidth="626.0" layoutX="547.0" layoutY="160.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../img/France.jpg" />
                           </image>
                        </ImageView>
                        <Label alignment="CENTER" layoutX="545.0" layoutY="46.0" style="-fx-text-fill: #4444FE;" text="France">
                           <font>
                              <Font name="System Bold" size="36.0" />
                           </font>
                        </Label>
                        <Label layoutX="53.0" layoutY="153.0" prefHeight="27.0" prefWidth="458.0" text="Little country located on Europe. Thanks to this page" wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="54.0" layoutY="192.0" prefHeight="27.0" prefWidth="458.0" text="you will learn few things about it. France is a transcon-" wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="53.0" layoutY="232.0" prefHeight="27.0" prefWidth="458.0" text="tinental sovereign state, whose metropolitan territory" wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="53.0" layoutY="309.0" prefHeight="27.0" prefWidth="458.0" text="ers with Belgium, Luxembourg, Germany, Switzerland, " wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="54.0" layoutY="433.0" prefHeight="27.0" prefWidth="458.0" text="Atlantic, the Manche, the North Sea and the Mediter-" wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="54.0" layoutY="473.0" prefHeight="27.0" prefWidth="458.0" text="ranean. Its overseas territory extends into the Indian," wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="53.0" layoutY="391.0" prefHeight="27.0" prefWidth="458.0" text="Monaco, and has important maritime facades in the" wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="54.0" layoutY="348.0" prefHeight="27.0" prefWidth="458.0" text="Italy, Spain and the principalities of Andorra and" wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="53.0" layoutY="271.0" prefHeight="27.0" prefWidth="458.0" text="is located in Western Europe. The latter has land bord-" wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="53.0" layoutY="514.0" prefHeight="27.0" prefWidth="458.0" text="Atlantic and Pacific oceans as well as on the South" wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="54.0" layoutY="559.0" prefHeight="27.0" prefWidth="458.0" text="American continent, and has land borders with Brazil," wrapText="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
